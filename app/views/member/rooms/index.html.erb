<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Type</th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%=h room.room_type %></td>
    <td><%= link_to 'Show', member_room_path(room) %></td>
    <td><%= link_to 'Edit', edit_member_room_path(room) %></td>
    <td><%= link_to 'Destroy', member_room_path(room), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New room', new_member_housing_room_path %>