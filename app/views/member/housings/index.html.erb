<h1>Listing housings</h1>

<table>
  <tr>
    <th>Hebergement</th>
    <th>Address</th>
    <th>User</th>
    <th>Phone contact</th>
    <th>Email</th>
    <th>Website</th>
    <th>Name</th>
    <th>Is visible</th>
    <th>Type</th>
    <th>Url alias</th>
    <th>Distance commerce</th>
    <th>Distance train</th>
    <th>Distance transport</th>
    <th>Gmap latitude</th>
    <th>Gmap longitude</th>
  </tr>

<% @housings.each do |housing| %>
  <tr>
    <td><%= housing.housings_id %></td>
    <td><%= housing.address_id %></td>
    <td><%= housing.user_id %></td>
    <td><%= housing.phone_contact %></td>
    <td><%= housing.email %></td>
    <td><%= housing.website %></td>
    <td><%= housing.name %></td>
    <td><%= housing.is_visible %></td>
    <td><%#=h housing.type %></td>
    <td><%= housing.url_alias %></td>
    <td><%= housing.distance_commerce %></td>
    <td><%= housing.distance_train %></td>
    <td><%= housing.distance_transport %></td>
    <td><%#= housing.gmap_latitude %></td>
    <td><%#= housing.gmap_longitude %></td>
    <td><%= link_to 'Show', [get_module_name, housing] %></td>
    <td><%= link_to 'Edit', get_module_path("edit_%_housing", housing) %></td>
    <td><%= link_to 'Destroy', [get_module_name, housing], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New housing', get_module_path("new_%_housing") %>